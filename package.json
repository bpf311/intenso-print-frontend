{
  "name": "intenso-print",
  "version": "0.1.0",
  "author": "Brian D. Pillco Flores",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "app:build": "yarn build:for:electron && yarn electron:builder",
    "build:for:electron": "cross-env ELECTRON=true vue-cli-service build",
    "electron": "cross-env IS_DEV=true electron .",
    "electron:builder": "electron-builder"
  },
  "main": "electron/electron.js",
  "dependencies": {
    "axios": "^1.3.4",
    "core-js": "^3.8.3",
    "electron": "25.3.2",
    "electron-pos-printer": "1.3.6",
    "exceljs": "^4.3.0",
    "luxon": "^3.4.4",
    "moment": "^2.29.4",
    "print-js": "^1.6.0",
    "vee-validate": "3",
    "vue": "^2.6.14",
    "vue-router": "^3.5.1",
    "vue-toastification": "^1.7.14",
    "vuetify": "^2.6.0",
    "vuex": "^3.6.2",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-standard": "^6.1.0",
    "cross-env": "^7.0.3",
    "electron-builder": "^23.6.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^8.0.3",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "vue-cli-plugin-vuetify": "~2.5.8",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.0"
  },
  "build": {
    "appId": "com.my-website.my-app",
    "productName": "IntensoPrint",
    "copyright": "Copyright Â© 2023 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist_electron"
    }
  }
}
